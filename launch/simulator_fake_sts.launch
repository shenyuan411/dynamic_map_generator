<launch>
	<arg name="init_x" />
	<arg name="init_y" />
	<arg name="init_z" />
	<arg name="obs_num" default="1" />
	<arg name="map_size_x" default="5"/>
	<arg name="map_size_y" default="7"/>
	<arg name="map_size_z" default="3"/>
	<arg name="c_num" />
	<arg name="p_num" />
	<arg name="min_dist" />
	<arg name="odometry_topic" />
	<arg name="odom_pose_topic" />
	<arg name="drone_id" />
	<arg name="map_mode1" default="2" /> <!-- 障碍物1的运动模式为只有y方向的速度-->
	<arg name="given_vel1x" default="0.05"/>  <!-- 单位很迷阿  -->
	<arg name="given_vel1y" default="0.05"/>  <!-- 单位很迷阿  -->
	<arg name="map_mode4" default="0" /> <!-- 障碍物4的运动模式为有x和y方向的速度-->
	<arg name="given_vel4x" default="0.033"/>  <!-- 单位很迷阿  -->
	<arg name="given_vel4y" default="-0.02"/>  <!-- 单位很迷阿  -->
	<arg name="map_mode5" default="1" /> <!-- 障碍物4的运动模式为有x和y方向的速度-->
	<arg name="given_vel5x" default="0.04"/>
	<arg name="given_vel5y" default="-0.02"/>

	<!-- map -->
	<node pkg ="map_generator" name ="map_generator" type ="dynamic_walker" output = "screen">
		<remap from="~global_cloud" to="/map_generator/global_cloud" />
		<remap from="~global_cylinder_state" to="/ground_truth_state" />
		<param name="map/x_size" value="$(arg map_size_x)" />
		<param name="map/y_size" value="$(arg map_size_y)" />
		<param name="map/z_size" value="$(arg map_size_z)" />
		<param name="map/resolution" value="0.1"/>
		<param name="map/frame_id" value="world" />
		<param name="map/test" value="false" />

		<param name="ObstacleShape/seed" value="-1"/>
		<param name="map/obs_num" value="5"/>
		<!-- 改变障碍物体积 -->
		<param name="ObstacleShape/lower_rad" value="0.5"/>
		<param name="ObstacleShape/upper_rad" value="1.0"/>
		<param name="ObstacleShape/lower_hei" value="0.0"/>
		<param name="ObstacleShape/upper_hei" value="2.0"/>
		<param name="ObstacleShape/upper_vel" value="0.6" />
		<param name="ObstacleShape/set_cylinder" value="true"/>

		<param name="map/circle_num" value="0"/>
		<param name="ObstacleShape/radius_l" value="0.7"/>
		<param name="ObstacleShape/radius_h" value="2.5"/>
		<param name="ObstacleShape/z_l" value="0.8"/>
		<param name="ObstacleShape/z_h" value="2.0"/>
		<param name="ObstacleShape/dr" value="0.0"/>
		<param name="ObstacleShape/theta" value="0.2"/>

		<param name="sensing/radius" value="10.0"/>
		<param name="sensing/rate" value="20.0"/>
		<param name="obs1x" value="0.0"/>
		<param name="obs1y" value="0.0"/>
		<param name="obs1w" value="0.7"/> <!-- 圆柱的宽度 -->
		<param name="mode1" value="$(arg map_mode1)" />
		<param name="given_vel1x" value="$(arg given_vel1x)"/>
		<param name="given_vel1y" value="$(arg given_vel1y)"/>
		<param name="obs2x" value="-2.0"/>
		<param name="obs2y" value="1.5"/>
		<param name="obs2w" value="1.34"/>
		<param name="obs3x" value="5.0"/>
		<param name="obs3y" value="-1.5"/>
		<param name="obs3w" value="1.4"/>
		<param name="obs4x" value="-0.4"/>
		<param name="obs4y" value="0.0"/>
		<param name="obs4w" value="0.7"/>
		<param name="mode4" value="$(arg map_mode4)" />
		<param name="given_vel4x" value="$(arg given_vel4x)"/>
		<param name="given_vel4y" value="$(arg given_vel4y)"/>
		<param name="obs5x" value="1.0"/>
		<param name="obs5y" value="0.0"/>
		<param name="obs5w" value="0.7"/>
		<param name="mode5" value="$(arg map_mode5)" />
		<param name="given_vel5x" value="$(arg given_vel5x)"/>
		<param name="given_vel5y" value="$(arg given_vel5y)"/>


	</node>
</launch>


